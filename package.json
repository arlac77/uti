{
  "name": "uti",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "main": "dist/module.js",
  "module": "src/uti.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/module.js -c -- src/uti.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^3.0.0",
    "rollup": "^0.41.5",
    "nyc": "^10.1.2"
  },
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0-semantic-release",
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}