{
  "name": "uti",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "main": "index.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "./node_modules/.bin/jsdoc index.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "./node_modules/rollup/bin/rollup --output=index.js -c -- {{module}}",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f {{module}} > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "2.11.15",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "markdown-doctest": "^0.9.0",
    "mocha": "^3.1.2",
    "semantic-release": "^6.3.2",
    "jsdoc-to-markdown": "^2.0.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0-semantic-release",
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ]
}