{
  "name": "uti",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/uti.mjs",
  "module": "src/uti.mjs",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/uti.mjs --section=API",
    "lint": "documentation lint src/uti.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^7.0.0",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "semantic-release": "^15.14.0"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ],
    "require": [
      "esm"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
