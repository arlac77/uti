{
  "name": "uti",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/module.js",
  "module": "src/uti.js",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "docs": "documentation readme src/uti.js --section=API",
    "semantic-release": "semantic-release",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/uti.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "c8": "^3.2.1",
    "documentation": "^8.1.2",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.10.8",
    "travis-deploy-once": "^5.0.9"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js"
    ],
    "presets": []
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
