{
  "name": "uti",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "main": "index.js",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "engines": {
    "node": ">=5"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "coveralls": "2.11.6",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^1.1.5",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "mocha": "2.4.5",
    "semantic-release": "6.2.0"
  },
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}