{
  "name": "uti",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/uti.mjs",
  "module": "src/uti.mjs",
  "description": "javascript implementation of a \"Uniform Type Identifier\" (UTI)",
  "keywords": [
    "MIME",
    "UTI",
    "file type"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/uti.mjs --section=API",
    "lint": "documentation lint src/uti.mjs",
    "test": "ava && ava --timeout 2m"
  },
  "devDependencies": {
    "ava": "^3.3.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "semantic-release": "^17.0.2"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/uti.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/uti/issues"
  },
  "homepage": "https://github.com/arlac77/uti#readme",
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
